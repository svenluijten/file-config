{
    "name": "sven/file-config",
    "description": "Store and read configuration values using files on disk",
    "keywords": ["config", "file", "flatfile", "configuration", "flysystem", "json"],
    "license": "MIT",
    "authors": [
        {
            "name": "Sven Luijten",
            "email": "svenluijten1995@gmail.com",
            "homepage": "https://svenluijten.com"
        }
    ],
    "require": {
        "php": "^7.1",
        "dflydev/dot-access-data": "^2.0",
        "league/flysystem": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "Sven\\FileConfig\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sven\\FileConfig\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    }
}
